pipeline {
    agent any
    stages {
        stage ('Git clone') {
            steps {
                git url: "git@github.com/Anand1294/sample-repo3", branch: "main", credentialsId: "git"
            }
        }
        stage ('Shell Script') {
            steps {
                script {
                    bat "type README.md"
                }
            }
        }
    }
}
